{
    "version": 2,
    "builds": [
        {
            "src": "nuxt.config.ts",
            "use": "@vercel/node"
        },
        {
            "src": "server/api/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/server/api/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "/.output/server/index.mjs"
        }
    ]
}